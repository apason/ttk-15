; count next prime into stack
; takes argument in r1
nextprime push sp,r5
push sp,r4
push sp,r3
push sp,r2
; find the next prime
loop add r1,=1
	; call sqrt function with r1 as parameter
	push sp,r1
	call sp,=sqrt
	load r4,r1
	pop sp,r1
	; start checking if r1 is a prime
	load r3,=2
	load r2,=1
	innerloop load r5,r1
		mod r5,r3
		mul r2,r5
		comp r2,=0
		jequ =loop
		comp r4,r3
		add r3,=1
		jgre =innerloop
pop sp,r2
pop sp,r3
pop sp,r4
pop sp,r5
exit sp,=0

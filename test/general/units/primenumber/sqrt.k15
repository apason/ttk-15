; squareroot with newtonian
; takes argument in r1
sqrt push sp,r4
	push sp,r3
	push sp,r2
	push sp,r0
	load r2,r1
	shr r2,=1
	load r0,=0
	loop add r0,=1
		load r3,r2
		load r4,r1
		div r4,r2
		add r2,r4
		shr r2,=1
		comp r0,=20
		jles loop
	load r1,r2
	pop sp,r0
	pop sp,r2
	pop sp,r3
	pop sp,r4
	exit sp,=0

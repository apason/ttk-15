; squareroot with newtonian
; takes argument in r1
; stores result in r1
export sqrt push sp,r4
	push sp,r2
	push sp,r0
	load r2,=1
;	load r2,r1
;	shr r2,=1	; r2 = x/2
	load r0,=0
	;; do 20 iterations of sqrt
	loop add r0,=1
		load r4,r1	
		div r4,r2	; x = x/x0
		add r2,r4	; x0 = x/x0
		shr r2,=1	; x0 = x0/2
		comp r0,=20
		jles =loop
	load r1,r2
	pop sp,r0
	pop sp,r2
	pop sp,r4
	exit sp,=0
